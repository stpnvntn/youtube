@keyframes dot-jump {
  0% {
    transform translateY(0px)
  }
  30% {
    transform translateY(-5px)
  }
  60% {
    transform translateY(-3px)
  }
  90% {
    transform translateY(-7px)
  }
  100% {
    transform translateY(0px)
  }
}

@keyframes dot-wrapper-fadein {
  from {
    opacity 0
  }
  to{
    opacity 100;
  }
}